{
  "name": "electron-forge-react-vite-boilerplate",
  "productName": "EFRV Boilerplate",
  "version": "1.1.0",
  "description": "Electron-Forge React Vite Boilerplate.",
  "main": ".vite/build/main.js",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "check": "biome check src/**/*.ts",
    "clean": "rimraf node_modules .vite pnpm-lock.yaml",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "keywords": ["electron", "react", "typescript", "boilerplate", "electron-forge", "vite"],
  "author": {
    "name": "Flavio Del Grosso"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flaviodelgrosso/electron-forge-react-typescript-boilerplate.git"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "electron-window-state": "^5.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/shared-types": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "4.5.1",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "electron": "36.4.0",
    "electron-devtools-installer": "^4.0.0",
    "husky": "9.1.7",
    "lint-staged": "^16.1.0",
    "npm-run-all": "^4.1.5",
    "pinst": "^3.0.0",
    "rimraf": "^6.0.1",
    "sass": "1.89.1",
    "ts-node": "10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.9.3",
    "vite-plugin-svgr": "4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
